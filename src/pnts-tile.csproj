<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>Pnts.Tile</RootNamespace>
    <AssemblyName>pnts.tile</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.2</Version>
    <Authors>Bert Temme</Authors>
    <Company>Geodan</Company>
    <Product>pnts-tile</Product>
    <PackageId>pnts-tile</PackageId>
    <Description>.NET Library for (de)serializing Pnts files</Description>
    <PackageLicenseUrl>https://github.com/bertt/pnts-tile-cs/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/bertt/pnts-tile-cs</PackageProjectUrl>
    <PackageTags>pnts tile pointcloud</PackageTags>
    <PackageReleaseNotes>adding colors</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="8.0.3" />
  </ItemGroup>

</Project>
